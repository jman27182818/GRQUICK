Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["M", "2"], " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "2"}], " ", "M"}], "+", "R"}], ")"}], "2"]}], 
  SuperscriptBox["R", "6"]]], "Output",
